#!/bin/ksh

db2 connect to db2data > /dev/null

echo "[DB2] Drop Tables..."
db2 "DROP TABLE BALANCESHEET"
db2 "DROP TABLE EMPLOYEE"
db2 "DROP TABLE INCOMESTATEMENT"
db2 "DROP TABLE ORDERSET" 
db2 "DROP TABLE PRODUCT"
db2 "DROP TABLE CUSTOMER"
db2 "DROP TABLE ORDER"
db2 "DROP TABLE PRESCRIPTION"
sleep 1
db2 "DROP TABLE EQUITY"

echo "[DB2] Drop Tables: DONE"
